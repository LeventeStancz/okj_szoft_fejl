                    <Button x:Name="Fooldal_btn" Content="Fő oldal" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="Fooldal_btn_Click"/>
                    <Button x:Name="BeosztasTervezes_btn" Content="Beosztás tervezés" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="BeosztasTervezes_btn_Click"/>
                    <Button x:Name="Fuvarok_btn" Content="Fuvarok" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="Fuvarok_btn_Click"/>
                    <Button x:Name="Felhasznalok_btn" Content="Felhasználók kezelése" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="Felhasznalok_btn_Click"/>
                    <Button x:Name="Jarmuvek_btn" Content="Járművek kezelése" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="Jarmuvek_btn_Click"/>
                    <Button x:Name="Szervizek_btn" Content="Szervízek kezelése" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="Szervizek_btn_Click"/>
                    <Button x:Name="Szervizeles_btn" Content="Szervizelés alatt" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Click="Szervizeles_btn_Click" Width="300"/>
                    <Button x:Name="SajatProfil_btn" Content="Saját profil" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Width="300" Click="SajatProfil_btn_Click"/>
                    <Button x:Name="Kijelentkezes_btn" Content="Kijelentkezés" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF0A0A0A" FontSize="24" FontWeight="Bold" Background="#00000000" HorizontalContentAlignment="Left" Padding="4,0,0,0" Click="Kijelentkezes_btn_Click" Width="300"/>
                </StackPanel>
            </ScrollViewer>
        </Grid>
        <Grid x:Name="Main_Grid" Margin="300,30,0,0">
            <Grid x:Name="Fooldal_Grid" Margin="10,0,10,10">
                <Label x:Name="kereses_label" Content="Keresés:" FontSize="32" VerticalContentAlignment="Center" Padding="0" Height="60" VerticalAlignment="Top" Background="#00000000" Foreground="#FFEEEEEE" HorizontalAlignment="Left" Width="600" Margin="10,0,0,0"/>
                <TextBox x:Name="Kereses_tb" Margin="10,65,615,0" TextWrapping="Wrap" VerticalContentAlignment="Center" FontSize="24" BorderBrush="{x:Null}" Background="#FFB9B9B9" SelectionBrush="#FFCDCDCD" Foreground="#FF023CFF" BorderThickness="2" Height="55" VerticalAlignment="Top" MaxLength="38"/>
                <Button x:Name="Kereses_btn" Content="Keresés" Height="55" Margin="0,65,10,0" VerticalAlignment="Top" BorderBrush="{x:Null}" Foreground="#FF151515" Background="#FFB9B9B9" FontSize="26" HorizontalAlignment="Right" Width="180" BorderThickness="2" Click="Kereses_btn_Click" Style="{DynamicResource KeresoBtn}"/>
                <Label x:Name="talalatok_label" Content="Találatok:" Margin="10,125,0,0" FontSize="28" VerticalContentAlignment="Center" Padding="2,0,0,0" Height="60" VerticalAlignment="Top" Background="#00000000" Foreground="#FFEEEEEE" HorizontalAlignment="Left" Width="600"/>
                <ComboBox x:Name="KeresoTabla_cb" SelectionChanged="KeresoTabla_cb_SelectionChanged" Height="55" Margin="0,65,410,0" VerticalAlignment="Top" FontSize="22" VerticalContentAlignment="Center" HorizontalAlignment="Right" Width="200"/>
                <ComboBox x:Name="KeresoMezo_cb" Height="55" Margin="0,65,195,0" VerticalAlignment="Top" FontSize="20" VerticalContentAlignment="Center" HorizontalAlignment="Right" Width="210"/>
                <DataGrid x:Name="dg" IsReadOnly="True" RowHeaderWidth="0" Background="#FF004B53" FontSize="24" BorderBrush="{x:Null}" SelectionUnit="Cell" Margin="10,190,10,10"/>
            </Grid>
            <Grid x:Name="SajatProfil_Grid" Visibility="Collapsed">
                <ScrollViewer x:Name="sajatprof__ScrollViewer" Margin="340,20,0,0" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
